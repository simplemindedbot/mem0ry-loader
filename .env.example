# Mem0 Configuration
MEM0_API_KEY=your_mem0_api_key_here

# OpenAI Configuration (Optional - for cloud processing)
MEMLOADER_OPENAI_API_KEY=your_openai_api_key_here
MEMLOADER_OPENAI_MODEL=gpt-4o-mini
MEMLOADER_LLM_PROVIDER=ollama

# Ollama Configuration
MEMLOADER_OLLAMA_BASE_URL=http://localhost:11434
MEMLOADER_OLLAMA_MODEL=nuextract

# Processing Configuration
MEMLOADER_CONFIDENCE_THRESHOLD=0.7
MEMLOADER_BATCH_SIZE=100
MEMLOADER_CHUNK_SIZE=1500
MEMLOADER_CHUNK_OVERLAP=200

# Rate Limiting
MEMLOADER_REQUESTS_PER_MINUTE=60
MEMLOADER_RETRY_ATTEMPTS=3
MEMLOADER_RETRY_DELAY=1.0

# Timeout Settings (in seconds)
MEMLOADER_OLLAMA_TIMEOUT=600